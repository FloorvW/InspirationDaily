aAAA,EACA,uDAFA;AAGA,4BAAA;AAHA;AADA,OAHA,EAWA,IAXA,CAWA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,MAAA;AACA,OAdA,EAeA,KAfA,CAeA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,EAAA;AACA,OAjBA;AAkBA,KA7CA;AA8CA,IAAA,WA9CA,uBA8CA,CA9CA,EA8CA;AACA;AACA,UAAA,IAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA,EAAA;AACA,UAAA,GAAA,GAAA,EAAA;AACA,UAAA,KAAA,GAAA,EAAA;AACA,UAAA,QAAA,GAAA,EAAA;AAEA,UAAA,EAAA,GAAA,CAAA,CAAA,UAAA,CAAA,EAAA;;AARA,iDASA,KAAA,MATA;AAAA;;AAAA;AASA,4DAAA;AAAA,cAAA,KAAA;;AACA,cAAA,KAAA,CAAA,EAAA,KAAA,EAAA,EAAA;AACA,YAAA,IAAA,GAAA,KAAA,CAAA,WAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,YAAA,IAAA,GAAA,KAAA,CAAA,IAAA,CAAA,IAAA;AACA,YAAA,GAAA,GAAA,KAAA,CAAA,IAAA,CAAA,GAAA;AACA,YAAA,KAAA,GAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,YAAA,QAAA,GAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AAEA;AACA;AAnBA;AAAA;AAAA;AAAA;AAAA;;AAoBA,WAAA,KAAA,CAAA,aAAA,EAAA,IAAA,EAAA,IAAA,EAAA,GAAA,EAAA,KAAA,EAAA,QAAA;AACA;AAnEA;AAZA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"button-wrapper\">\n      <button class=\"btn\" @click=\"searchUnsplash('Fashion')\">Fashion</button>\n      <button class=\"btn\" @click=\"searchUnsplash('Architecture')\">Architecture</button>\n      <button class=\"btn\" @click=\"searchUnsplash('Food')\">Food</button>\n      <button class=\"btn\" @click=\"searchUnsplash('Design')\">Design</button>\n    </div>\n    <div>\n      <h3>Sort by color:</h3>\n      <button @click=\"searchBlackWhite('black_and_white')\">Black and white</button>\n      <button @click=\"searchBlackWhite('black')\">Black</button>\n      <button @click=\"searchBlackWhite('white')\">White</button>\n      <button @click=\"searchBlackWhite('yellow')\">Yellow</button>\n      <button @click=\"searchBlackWhite('orange')\">Orange</button>\n      <button @click=\"searchBlackWhite('red')\">Red</button>\n      <button @click=\"searchBlackWhite('purple')\">Purple</button>\n      <button @click=\"searchBlackWhite('magenta')\">Magenta</button>\n      <button @click=\"searchBlackWhite('green')\">Green</button>\n      <button @click=\"searchBlackWhite('teal')\">Teal</button>\n      <button @click=\"searchBlackWhite('blue')\">Blue</button>\n    </div>\n    <stack :column-min-width=\"320\" monitor-images-loaded>\n      <stack-item\n        v-for=\"(image, i) in this.images\"\n        :key=\"i\"\n        style=\"transition: transform 300ms\"\n        @click.native=\"handleClick\"\n      >\n        <img :src=\"image.urls.small\" :alt=\"image.alt_description\" :id=\"image.id\" />\n      </stack-item>\n    </stack>\n  </div>\n</template>\n\n\n<script>\nimport axios from \"axios\";\nimport { Stack, StackItem } from \"vue-stack-grid\";\n\nexport default {\n  components: {\n    Stack,\n    StackItem,\n  },\n  data: () => ({\n    images: [],\n    image: {\n      description: \"\",\n    },\n    topic: \"\"\n  }),\n  methods: {\n    searchUnsplash(topic) {\n      this.images = [];\n      this.topic = topic;\n      \n      axios\n        .get(\n          `https://api.unsplash.com/search/photos?query=${topic}&per_page=10&order_by=latest`,\n          {\n            headers: {\n              Authorization:\n                \"Client-ID NzvzFaES6WCBY0qwjArIc45aNNnZRQDajC1CzRtYbO8\",\n              \"Accept-Version\": \"v1\",\n            },\n          }\n        )\n        .then((response) => {\n          this.images = response.data.results;\n          console.log(this.images);\n        })\n        .catch(() => {\n          this.images = [];\n        });\n    },\n    searchBlackWhite(color) {\n      this.images = [];\n\n      axios\n        .get(\n          `https://api.unsplash.com/search/photos?query=${this.topic}&per_page=10&order_by=latest&color=${color}`,\n          {\n            headers: {\n              Authorization:\n                \"Client-ID NzvzFaES6WCBY0qwjArIc45aNNnZRQDajC1CzRtYbO8\",\n              \"Accept-Version\": \"v1\",\n            },\n          }\n        )\n        .then((response) => {\n          this.images = response.data.results;\n          console.log(this.images);\n        })\n        .catch(() => {\n          this.images = [];\n        });\n    },\n    handleClick(e) {\n      // console.log(e)\n      var desc = \"\";\n      var name = \"\";\n      var bio = \"\";\n      var photo = \"\";\n      var username = \"\";\n\n      var id = e.srcElement.id;\n      for (var image of this.images) {\n        if (image.id === id) {\n          desc = image.description;\n          console.log(desc);\n          name = image.user.name;\n          bio = image.user.bio;\n          photo = image.urls.small;\n          username = image.user.username;\n          \n        }\n      }\n      this.$emit(\"handleClick\", desc, name, bio, photo, username);\n    },\n  },\n};\n</script>\n\n<style>\n.nav-bar {\n  background-color: aquamarine;\n  height: 60px;\n  margin-bottom: 15px;\n  width: 100%;\n  /* position: fixed; */\n}\n\n.container {\n  width: 80vw;\n  margin: 0 auto;\n}\n.button-wrapper {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 25px;\n  padding-top: 200px ;\n}\n.btn {\n  font-size: 18px;\n  background-color: #42b983;\n  color: white;\n  padding: 10px 20px;\n  margin-left: auto;\n  margin-right:auto;\n}\n\n.btn:not(:last-child) {\n  margin-right: 10px;\n}\n\n.img {\n  width: 100%;\n  height: auto;\n  border-radius: 12px;\n}\n</style>"],"sourceRoot":"src/views"}]}