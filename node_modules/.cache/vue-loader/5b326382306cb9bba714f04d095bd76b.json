tem,\n  },\n  data: () => ({\n    images: [],\n    image: {\n      description: \"\",\n    },\n  }),\n  methods: {\n    searchUnsplash() {\n      this.images = [];\n\n      axios\n        .get(`https://api.unsplash.com/photos?order_by=latest`, {\n          headers: {\n            Authorization:\n              \"Client-ID NzvzFaES6WCBY0qwjArIc45aNNnZRQDajC1CzRtYbO8\",\n            \"Accept-Version\": \"v1\",\n          },\n        })\n        .then((response) => {\n          console.log(response.data);\n          this.images = response.data;\n        })\n        .catch(() => {\n          this.images = [];\n        });\n    },\n    handleClick(e) {\n      // console.log(e)\n      var desc = \"\";\n      var name = \"\";\n      var bio = \"\";\n      var photo = \"\";\n      var username = \"\";\n     \n\n      var id = e.srcElement.id;\n      for (var image of this.images) {\n        if (image.id === id) {\n          desc = image.description;\n          console.log(\"Home Description :\"+desc);\n          name = image.user.name;\n          bio = image.user.bio;\n          photo = image.urls.small;\n          username = image.user.username;\n        }\n      }\n      this.$emit(\"handleClick\", desc, name, bio, photo, username);\n    },\n  },\n\n  mounted() {\n    this.searchUnsplash();\n  },\n};\n</script>\n\n<style scoped>\n.container{\n  padding-top: 18rem;\n}\n</style>"]}]}